<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title><%= title %></title>
</head>
<body>
    <% include menu %>
    
    <% entries.forEach(function(entry) { %>
        <div>
            <h3><%= entry.title %></h3>
            <p><%= entry.body %></p>
            <p>Posted by <%= entry.username %></p>
        </div>
    <% }) %>
</body>
</html>